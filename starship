[character]
success_symbol = "[➜](bold green)"
error_symbol = "[➜](bold red) "

[username]
style_user = "bold #ffb6c1"
show_always = true
format = "[$user]($style)@"

[hostname]
ssh_only = false
disabled = false
style = "bold #55fff"
format = "[$hostname]($style) | "

[cmd_duration]
min_time = 1
show_milliseconds = false
disabled = false
style = "bold dimmed #26ff8f"
format = "\\[[$duration]($style)\\]"

[directory]
truncation_length = 10
truncate_to_repo = true
style = "bold #87ceeb"
read_only =":lock:"
read_only_style = "bold red"
format = "[$read_only]($read_only_style)[$path]($style) "

[git_branch]
symbol = " "
truncation_length = 4
truncation_symbol = ""
style = "bold green"

[package]
style = "bold dimmed #e52fd7"
format = "[$symbol$version]($style) "
symbol = ":package: "

[git_status]
style = "bold dimmed white"

[python]
symbol = "Python "

[java]
symbol = "Java "
style = "red"

[nodejs]
symbol = "Node.js "
